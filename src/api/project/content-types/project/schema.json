{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "topMedia": {
      "type": "dynamiczone",
      "components": [
        "project-top-media.top-media",
        "top-media-video.top-media-video",
        "top-media-carousel.top-media-carousel"
      ],
      "required": true,
      "min": 1,
      "max": 1
    },
    "about": {
      "displayName": "about",
      "type": "component",
      "repeatable": false,
      "component": "project-about.about",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "projects"
    },
    "afterBefore": {
      "displayName": "afterBefor",
      "type": "component",
      "repeatable": false,
      "component": "project-after-befor.after-befor",
      "required": true
    },
    "video": {
      "displayName": "video",
      "type": "component",
      "repeatable": false,
      "component": "project-video.video",
      "required": true
    },
    "images": {
      "displayName": "images",
      "type": "component",
      "repeatable": false,
      "component": "project-images.images",
      "required": true
    },
    "backstage": {
      "displayName": "backstage",
      "type": "component",
      "repeatable": false,
      "component": "project-back-stage.backstage",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "bottomMailto": {
      "displayName": "mailto",
      "type": "component",
      "repeatable": false,
      "component": "mail-to.mailto",
      "required": true
    }
  }
}
