{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "project"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "topMedia": {
      "type": "dynamiczone",
      "components": [
        "project.top-media",
        "project.video",
        "project.carousel"
      ],
      "required": true,
      "min": 1,
      "max": 1
    },
    "aboutSection": {
      "displayName": "aboutSection",
      "type": "component",
      "repeatable": false,
      "component": "project.about-section"
    },
    "afterBefore": {
      "displayName": "afterBefore",
      "type": "component",
      "repeatable": false,
      "component": "project.after-before",
      "required": true
    },
    "videoSection": {
      "displayName": "videoSection",
      "type": "component",
      "repeatable": false,
      "component": "project.video-section"
    },
    "imagesSection": {
      "displayName": "imagesSection",
      "type": "component",
      "repeatable": false,
      "component": "project.images-section"
    },
    "backStageSection": {
      "displayName": "backStageSection",
      "type": "component",
      "repeatable": false,
      "component": "project.back-stage-section"
    },
    "bottomMailTo": {
      "displayName": "bottomMailTo",
      "type": "component",
      "repeatable": false,
      "component": "project.bottom-mail-to"
    }
  }
}
